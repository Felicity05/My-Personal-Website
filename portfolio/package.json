{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "functional-test": "start-server-and-test serve http://localhost:8080 integrated-test",
    "integrated-test": "cypress run",
    "unit-test": "jest"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "aos": "^2.3.4",
    "core-js": "^2.6.11",
    "flush-promises": "^1.0.2",
    "jest-serializer-vue": "^2.0.2",
    "jquery": "^3.5.1",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "scroll-out": "^2.2.12",
    "vue": "^2.6.12",
    "vue-pdf": "^4.2.0",
    "vue-router": "^3.4.9",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/test-utils": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "cypress": "^3.8.3",
    "deepmerge": "^4.2.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.9.0",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.11.5",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    }
  }
}
